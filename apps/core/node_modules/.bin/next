#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/dist/bin/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/dist/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/dist/bin/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/dist/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules/next/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/next@15.1.7_react-dom@19.0.0_react@19.0.0__react@19.0.0_sass@1.85.0/node_modules:/home/kg/Documents/repos/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
